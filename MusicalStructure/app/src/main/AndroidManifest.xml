<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.madhavbangaru.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FavoritesActivity"
            android:label="Favourites"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".LatestActivity"
            android:label="Latest"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".PopularActivity"
            android:label="Popular"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".PlaylistActivity"
            android:label="Playlist"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".PlayScreenActivity"
            android:label="Now Playing"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>